<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="adminVBox"
      alignment="CENTER"
      spacing="10.0"
      xmlns="http://javafx.com/javafx/21"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.aoopproject.controllers.admin.AdminController">
    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>
    <Button fx:id="notificationsButton" text="Notifications"/>
    <Button fx:id="notesButton" text="Notes"/>
    <Button fx:id="timerButton" text="Timer"/>
    <Button fx:id="doubtsolvingButton" text="Doubt Solving"/>
    <Button fx:id="logoutButton" text="Logout" onAction="#handleLogoutButtonAction" />
</VBox>